%h3
  Dashboard
.row
  .col-md-6
    .teams
      %h5
        My Teams
      = render(partial: "available_team", collection: current_user.available_teams)
  .col-md-6
    %h5
      My Information
    .user-information
      .email
        %strong
          Email:
        = current_user.email
      .sign-in-option
        = render(partial: "form")
